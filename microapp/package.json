{
  "name": "HikeSketch",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "install:47": "rm -rf node_modules && npm install",
    "build:and": "mkdir -p dist/and/hikesketch && rm -rf dist/and/hikesketch/* && react-native bundle --entry-file index.android.js --dev false --platform android --bundle-output dist/and/hikesketch/app.bundle --assets-dest dist/and/hikesketch/ && cp images/icon.png dist/and/hikesketch/",
    "build:ios": "mkdir -p dist/ios/hikesketch && rm -rf dist/ios/hikesketch/* && react-native bundle --entry-file index.ios.js --dev false --platform ios --bundle-output dist/ios/hikesketch/app.bundle --assets-dest dist/ios/hikesketch/ && cp images/icon.png dist/ios/hikesketch/"
  },
  "dependencies": {
    "asyncstorage-down": "^4.0.1",
    "base64-to-image": "^1.0.2",
    "fs": "0.0.1-security",
    "hikereactsdk": "^1.3.18",
    "keymirror": "0.1.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.0",
    "react-native-animatable": "^1.1.0",
    "react-native-camera-roll-picker": "1.2.0",
    "react-native-cropimage": "^1.1.1",
    "react-native-datepicker": "^1.6.0",
    "react-native-extended-stylesheet": "0.3.2",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-level-fs": "^3.0.0",
    "react-native-router-flux": "3.38.0",
    "react-native-signature-pad": "0.0.8",
    "react-native-theme": "^0.1.8",
    "react-navigation": "^1.0.0-beta.12",
    "react-redux": "^4.4.5",
    "redux": "3.5.2",
    "redux-thunk": "2.2.0",
    "underscore": "^1.8.3"
  },
 
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "3.0.2",
    "babel-preset-react-optimize": "^1.0.1",
    "jest": "20.0.4",
    "react-test-renderer": "15.4.2"
  },
  "peerDependencies": {
    "asyncstorage-down": "^3.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
